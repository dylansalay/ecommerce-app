<div class="container inventory-container text-muted">
  <div class="col-md-12">
    <div class="row">
      <div class= "form col-sm-6 col-md-6">
        <%= simple_form_for @shopping_cart do |f| %>
          <div class="card">  
            <div class="form-group">
              <%= render partial: 'address_form', locals: { f: f } %>
            </div>
          </div>
          <%= f.button :submit, class: "btn btn-primary mt-4 pull-right" %>
        <% end %>
      </div>
    </div>
  </div>
</div>